SYSTEM
	RailwaysSystemL0_CONT
SETS
	RailwaysSystem;
	RailwaysSystemStates
CONSTANTS
	rs,
	disabled,
	enabled
PROPERTIES
	rs : RailwaysSystem &
	RailwaysSystem ={rs} &
	enabled : RailwaysSystemStates &
	disabled : RailwaysSystemStates &
	disabled /= enabled &
	RailwaysSystemStates ={enabled, disabled}
END

